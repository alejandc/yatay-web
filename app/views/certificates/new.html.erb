<div class="row-fluid">
  <div class="span12">
    
    <div class="row-fluid">
      <div class="span10 offset1">
        <h3>
          <%= t('.title')%>
        </h3>
      </div>
    </div>
    
    <div class="row-fluid">
      <div class="span7 offset1">
				<%= form_for @certificate, :html => { :class => 'form-horizontal', :style => "margin-bottom: 0px;"} do |f| %>
					<%= render :partial => 'form', :locals => {:f => f} %>
          
          <%= f.hidden_field :client_id, value: @client_id %>
          <%= hidden_field :certificate_id_refered, value: @certificate_id_refered %>
          
				  <div class="control-group">
            <div class="controls">
              <%= f.submit t('.button_create_execution'), :class => 'btn btn-primary' %>
              &nbsp; 
              <%= link_to t('.link_to_cancel'), :back, :class => 'btn' %>
            </div>
          </div>
				<% end %>
      </div>
    </div>
  </div>
</div>